{
	"name": "grade-lms",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"export": "next export",
		"format": "next lint --fix && prettier '**/*.{js,jsx,ts,tsx,json,yaml}' --write",
		"check-types": "tsc --noEmit --pretty && tsc --project tsconfig.json --noEmit --pretty",
		"build-prod": "run-s clean build export",
		"clean": "rimraf .next out",
		"prepare": "husky install",
		"supabase:start": "npx supabase start",
		"supabase:db:reset": "npx supabase db reset",
		"supabase:gen-types": "npx supabase gen types typescript --local --schema public > types/supabase.type.ts",
		"supabase:restart": "yarn supabase:db:reset && yarn supabase:gen-types"
	},
	"dependencies": {
		"@supabase/ssr": "^0.1.0",
		"@supabase/supabase-js": "^2.41.1",
		"clsx": "^2.1.0",
		"date-fns": "^3.6.0",
		"firebase": "^10.10.0",
		"next": "14.1.4",
		"next-nprogress-bar": "^2.3.11",
		"react": "^18",
		"react-dom": "^18",
		"react-hot-toast": "^2.4.1",
		"zod": "^3.22.4",
		"zustand": "^4.5.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@types/node": "^20",
		"@types/postcss-js": "^4.0.4",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.1.4",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.38",
		"postcss-js": "^4.0.1",
		"tailwindcss": "^3.3.0",
		"typescript": "^5"
	}
}
